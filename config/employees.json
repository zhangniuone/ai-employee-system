{
  "employees": [
    {
      "id": "demo-xiaohongshu",
      "name": "小红书运营专员",
      "role": "content_creator",
      "platform": "xiaohongshu",
      "enabled": false,
      "schedule": "0 9,15,21 * * *",
      "tasks": ["generate_post"],
      "config": {
        "topics": ["AI工具推荐", "效率提升技巧", "副业赚钱方法", "职场干货"],
        "style": "轻松幽默",
        "maxLength": 800
      }
    },
    {
      "id": "demo-wechat",
      "name": "公众号文案写手",
      "role": "content_creator",
      "platform": "wechat",
      "enabled": false,
      "schedule": "0 8 * * 1,3,5",
      "tasks": ["generate_post"],
      "config": {
        "topics": ["人工智能", "数字化转型", "个人成长"],
        "style": "专业深度",
        "maxLength": 2000
      }
    },
    {
      "id": "demo-xianyu",
      "name": "闲鱼自动客服",
      "role": "customer_service",
      "platform": "xianyu",
      "enabled": false,
      "schedule": "*/10 * * * *",
      "tasks": ["auto_reply"],
      "config": {
        "platforms": ["xianyu"],
        "autoReplyEnabled": true,
        "replyTemplates": {
          "greeting": "亲，欢迎咨询~ 这款商品还在哦",
          "price": "亲，价格可以小刀，诚心要的话私聊我",
          "shipping": "亲，48小时内发货，包邮哦"
        }
      }
    }
  ],
  "global": {
    "requestInterval": 3000,
    "maxRetry": 3,
    "notifyOnError": true,
    "logLevel": "info"
  }
}